<!-- db/changelog-1.1-container.xml -->

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

  <!-- Create Container Table -->
    <changeSet id="1" author="your-name">
    <createTable tableName="societe">
      <column name="id" type="BIGINT">
        <constraints primaryKey="true" nullable="false"/>
      </column>
        <column name="nom" type="VARCHAR(500)"/>
        <column name="date_creation" type="TIMESTAMP"/>
        <column name="rc" type="VARCHAR(500)"/>
        <column name="taxe_professionnelle" type="VARCHAR(500)"/>
        <column name="ice" type="VARCHAR(500)"/>
        <column name="gerant" type="VARCHAR(500)"/>

        <column name="type_id" type="BIGINT">
          <constraints nullable="false"/>
        </column>

    </createTable>
  </changeSet>

    <!-- Add foreign key constraint -->
  <changeSet id="2" author="your-name">
      <addForeignKeyConstraint
              baseTableName="societe"
              baseColumnNames="type_id"
              referencedTableName="type"
              referencedColumnNames="id"
              constraintName="fk_societe_type"/>

  </changeSet>

    <!-- Create Sequence for ContainerType -->
  <changeSet id="3" author="your-name">
    <createSequence sequenceName="societe_seq" startValue="1" incrementBy="1"/>
  </changeSet>

  <!-- Load Data from CSV -->
  <changeSet id="4" author="your-name">
    <loadData
            file="classpath:/db/migration/data/soc/Societe.csv"
            tableName="societe">
      <column name="id" type="NUMERIC"/>
          <column name="nom" type="STRING"/>
          <column name="date_creation" type="TIMESTAMP"/>
          <column name="rc" type="STRING"/>
          <column name="taxe_professionnelle" type="STRING"/>
          <column name="ice" type="STRING"/>
          <column name="gerant" type="STRING"/>
        <column name="type" type="BIGINT"/>
    </loadData>
  </changeSet>


</databaseChangeLog>
